{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "renergy-backoffice",
  "main": "src/index.tsx",
  "compatibility_date": "2025-10-31",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "renergy-backoffice-db",
      "database_id": "local-db-for-development"
    }
  ]
}